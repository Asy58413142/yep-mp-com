.yep-upload {
  display: flex;
  flex-wrap: wrap;
  margin-left: -6rpx;
  padding: 10rpx 0;
  background: #fff;
  border: 1rpx solid #efefef;
  border-radius: 10rpx;
}
.yep-img-block {
  position: relative;
  margin-left: 6rpx;
  margin-bottom: 6rpx;
}
.yep-img-block-pic {
  width: 100%;
  height: 100%;
}
.yep-img-close {
  position: absolute;
  top: 15rpx;
  right: 15rpx;
  width: 32rpx;
  height: 32rpx;
  opacity: 0.6;
  background: url(data:image/png;base64,);
  background-size: 32rpx 32rpx;
  border-radius: 50%;
}
.yep-upload .yep-img-list {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #f8f8f8;
  border: dashed 1px #999999;
  margin-bottom: 6rpx;
  margin-left: 6rpx;
  box-sizing: border-box;
}
.yep-img-list .yep-icon-w {
  position: absolute;
  width: 90rpx;
  height: 6rpx;
  border-radius: 5rpx;
  background-color: #bfbfbf;
  top: 50%;
  left: 50%;
  margin-top: -3rpx;
  margin-left: -45rpx;
}
.yep-img-list .yep-icon-h {
  position: absolute;
  width: 6rpx;
  height: 90rpx;
  border-radius: 4rpx;
  background-color: #bfbfbf;
  top: 50%;
  left: 50%;
  margin-top: -45rpx;
  margin-left: -3rpx;
}
